		"body": [
		  "int CountSetBits(int n)",
		  "{",
		  "    int i=0;",
		  "    vector<int> v;",
		  "    while(n>0) ",
		  "    {",
		  "        v.push_back(n%2);",
		  "        n/=2;",
		  "        i++;",
		  "    }",
		  "    int count=0;",
		  "    for(int i=0;i<v.size();i++)",
		  "    {",
		  "        if(v[i]==1)",
		  "            count++;",
		  "    }",
		  "    return count;",
		  "}"
